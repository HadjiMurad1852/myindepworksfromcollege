#include <iostream>

int main()
{
    using namespace std;
    setlocale(LC_ALL, "RU");
    int a;
    int b;
    int c;
    int d;
    int e;
    int r;
    int w;
    int u;
    int i;
    int s;
    int k;
    int l;
    int g;
    int q;
    int p;
    cout << "Введите число: ";
    cin >> a;
    cout << "Решите пример: " << a << " * 1" << endl;
    cout << "Введите ответ: ";
    cin >> b;
    if (1 * a == b) {
        cout << "Пример решен правильно!" << endl << "Введите число: ";
        cin >> c;
        cout << "Решите пример: " << c << " * 2" << endl << "Введите ответ: ";
        cin >> d;
        if (c * 2 == d) {
            cout << "Пример решен правильно!" << endl << "Введите число: ";
            cin >> e;
            cout << "Решите пример: " << e << " * 3" << endl << "Введите ответ: ";
            cin >> r;
            if (e * 3 == r) {
                cout << "Пример решен правильно!" << endl << "Введите число: ";
                cin >> w;
                cout << "Решите пример: " << w << " * 4" << endl << "Введите ответ: ";
                cin >> u;
                if (w * 4 == u) {
                    cout << "Пример решен правильно!" << endl << "Введите число: ";
                    cin >> i;
                    cout << "Решите пример: " << i << " * 5" << endl << "Введите ответ: ";
                    cin >> s;

                    if (i * 5 == s) {
                        cout << "Пример решен правильно!" << endl << "Введите число: ";
                        cin >> k;
                        cout << "Решите пример: " << k << " * 6" << endl << "Введите ответ: ";
                        cin >> l;
                        if (k * 6 == l) {
                            cout << "Пример решен правильно!" << endl << "Введите число: ";
                            cin >> k;
                            cout << "Решите пример: " << k << " * 7" << endl << "Введите ответ: ";
                            cin >> l;
                            if (k * 7 == l) {
                                cout << "Пример решен правильно!" << endl << "Введите число: ";
                                cin >> k;
                                cout << "Решите пример: " << k << " * 8" << endl << "Введите ответ: ";
                                cin >> l;
                                if (k * 8 == l) {
                                    cout << "Пример решен правильно!" << endl << "Введите число: ";
                                    cin >> k;
                                    cout << "Решите пример: " << k << " * 9" << endl << "Введите ответ: ";
                                    cin >> l;
                                    if (k * 9 == l) {
                                        cout << "Примеры решены правильно!";
                                    }
                                    else {
                                        cout << "Ошибка, пример решен неверно!";
                                    }





                                }
                                else {
                                    cout << "Ошибка, пример решен неверно!";
                                }

                            }
                            else {
                                cout << "Ошибка, пример решен неверно!";
                            }

                        }
                        else {
                            cout << "Ошибка, пример решен неверно!";
                        }

                    }
                    else {
                        cout << "Ошибка, пример решен неверно!";
                    }

                }
                else {
                    cout << "Ошибка, пример решен неверно!";
                }

            }
            else {
                cout << "Ошибка, пример решен неверно!";
            }

        }
        else {
            cout << "Ошибка, пример решен неверно!";
        }

    }
    else {
        cout << "Ошибка, пример решен неверно!";
    }
    cin >> p;
}

