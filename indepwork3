#include <iostream>

int main()
{
    using namespace std;
    setlocale(LC_ALL, "RU");
    int a;
    cout << "Введите число от 1 до 100: ";
    cin >> a;
    if (a >= 0 and a <= 10) {
        cout << "Цифра в диапозоне 0 - 10";
    }
    else if (a >= 11 and a <= 20) {
        cout << "Цифра в диапозоне 11 - 20";
    }
    else if (a >= 21 and a <= 30) {
        cout << "Цифра в диапозоне 21 - 30";
    }
    else if (a >= 31 and a <= 40) {
        cout << "Цифра в диапозоне 31 - 40";
    }
    else if (a >= 41 and a <= 50) {
        cout << "Цифра в диапозоне 41 - 50";
    }
    else if (a >= 51 and a <= 60) {
        cout << "Цифра в диапозоне 51 - 60";
    }
    else if (a >= 61 and a <= 70) {
        cout << "Цифра в диапозоне 61 - 70";
    }
    else if (a >= 71 and a <= 80) {
        cout << "Цифра в диапозоне 71 - 80";
    }
    else if (a >= 81 and a <= 90) {
        cout << "Цифра в диапозоне 81 - 90";
    }
    else if (a >= 91 and a <= 100) {
        cout << "Цифра в диапозоне 91 - 100";
    }
    else {
        cout << "Ошибка! Вы ввели цифру вне диапозона 0 - 100.";
    }
    
}
